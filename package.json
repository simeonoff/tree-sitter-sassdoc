{
  "name": "tree-sitter-sassdoc",
  "version": "0.1.0",
  "description": "SassDoc grammar for tree-sitter",
  "repository": "https://github.com/simeonoff/tree-sitter-sassdoc",
  "license": "MIT",
  "author": {
    "name": "Simeon Simeonoff",
    "email": "simeon@simeonoff.ninja",
    "url": "https://github.com/simeonoff"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "sassdoc",
    "scss",
    "sass",
    "documentation",
    "syntax-highlighting"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/**/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter": "^0.22.4",
    "tree-sitter-cli": "^0.25.10"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "generate": "tree-sitter generate",
    "build": "tree-sitter generate && node-gyp build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "tree-sitter test",
    "test:node": "node --test bindings/node/*_test.js"
  }
}
